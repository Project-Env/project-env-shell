<?xml version="1.0" encoding="UTF-8"?>
<project-env xmlns="ch.repolevedavaj.projectenv"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="ch.repolevedavaj.projectenv project-env.xsd">
    <tools>
        <directory>tools</directory>
        <installers>
            <jdk>
                <name>jdk</name>
                <download-uris>
                    <macos>https://github.com/graalvm/graalvm-ce-builds/releases/download/vm-20.3.0/graalvm-ce-java11-darwin-amd64-20.3.0.tar.gz</macos>
                    <windows>https://github.com/graalvm/graalvm-ce-builds/releases/download/vm-20.3.0/graalvm-ce-java11-windows-amd64-20.3.0.zip</windows>
                    <linux>https://github.com/graalvm/graalvm-ce-builds/releases/download/vm-20.3.0/graalvm-ce-java11-linux-amd64-20.3.0.tar.gz</linux>
                </download-uris>
                <post-installation-commands>
                    <post-installation-command>
                        <executable>gu</executable>
                        <arguments>
                            <argument>install</argument>
                            <argument>native-image</argument>
                        </arguments>
                    </post-installation-command>
                </post-installation-commands>
            </jdk>
            <maven>
                <name>maven</name>
                <download-uris>
                    <system-independent>https://downloads.apache.org/maven/maven-3/3.6.3/binaries/apache-maven-3.6.3-bin.tar.gz</system-independent>
                </download-uris>
            </maven>
            <gradle>
                <name>gradle</name>
                <download-uris>
                    <system-independent>https://downloads.gradle-dn.com/distributions/gradle-6.7.1-bin.zip</system-independent>
                </download-uris>
            </gradle>
            <node>
                <name>node</name>
                <download-uris>
                    <macos>https://nodejs.org/dist/v14.15.3/node-v14.15.3-darwin-x64.tar.gz</macos>
                    <windows>https://nodejs.org/dist/v14.15.3/node-v14.15.3-win-x64.zip</windows>
                    <linux>https://nodejs.org/dist/v14.15.3/node-v14.15.3-linux-x64.tar.xz</linux>
                </download-uris>
                <post-installation-commands>
                    <post-installation-command>
                        <executable>npm</executable>
                        <arguments>
                            <argument>install</argument>
                            <argument>-g</argument>
                            <argument>yarn</argument>
                        </arguments>
                    </post-installation-command>
                </post-installation-commands>
            </node>
            <generic>
                <name>jaxb-ri</name>
                <download-uris>
                    <system-independent>https://repo1.maven.org/maven2/com/sun/xml/bind/jaxb-ri/3.0.0/jaxb-ri-3.0.0.zip</system-independent>
                </download-uris>
                <exports>
                    <export>
                        <name>JAXB_HOME</name>
                        <value/>
                    </export>
                </exports>
                <path-elements>
                    <path-element>bin</path-element>
                </path-elements>
            </generic>
        </installers>
    </tools>
</project-env>